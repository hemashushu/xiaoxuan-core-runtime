import fn module::base::primitive::add(i32,i32)->i32
import fn module::base::primitive::mul(i32,i32)->i32

pub fn inc(num:i32) -> i32 {
    call(add
        local_load_i32_s(num),
        imm_i32(1)
    )
}

pub fn double(num:i32) -> i32 {
    call(mul
        local_load_i32_s(num)
        imm_i32(2)
    )
}